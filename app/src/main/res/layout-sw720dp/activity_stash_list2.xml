<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:baselineAligned="false">

    <fragment
        android:layout_width="0dp"
        android:layout_weight="2"
        android:layout_height="match_parent"
        android:id="@+id/activity_stash_list_frag"
        android:name="com.bombadu.stash.StashListFragment"
        tools:layout="@layout/activity_stash_list"/>


    <fragment
        android:layout_width="0dp"
        android:layout_weight="3"
        android:layout_height="match_parent"
        android:id="@+id/activity_web_view_frag"
        android:name="com.bombadu.stash.WebViewFragment"
        tools:layout="@layout/web_view_fragment"/>

</LinearLayout>